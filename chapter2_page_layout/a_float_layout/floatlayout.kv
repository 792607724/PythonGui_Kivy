<Button>:
    font_size:40
    size_hint:0.3,0.3
<FloatLayoutWidget>:
    canvas:
        Color:
            rgba:[1,1,1,1]
        Rectangle:
            size:self.size
            pos:self.pos
    Button:
        text:"1"
        background_color:0.1,0.5,0.6,1
        pos_hint:{"x":0,"top":1}
    Button:
        text:"2"
        background_color:1,0,0,1
        pos_hint:{"center_x":0.5,"center_y":0.5}
    Button:
        text:"3"
        background_color:0.4,0.5,0.6,1
        pos_hint:{"x":0.7,"bottom":0}
    Button:
        text:"4"
        background_color:0,0,1,1
        pos_hint:{"x":0,"bottom":1}
    Button:
        text:"5"
        background_color:0.5,0.6,0.7,1
        pos_hint:{"x":0.7, "top":1}